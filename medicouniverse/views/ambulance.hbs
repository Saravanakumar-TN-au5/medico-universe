        <div class="modal-dialog cascading-modal modal-avatar modal-sm" role="document">
            <!--Content-->
            <div class="modal-content">

                <!--Header-->
                <div class="modal-header">
                    <img src="https://res.cloudinary.com/dps7y8nd7/image/upload/v1579798959/Untitled1_kte8hz.jpg"
                        alt="avatar" class="rounded-circle img-responsive">
                </div>
                <!--Body-->
                <div class="modal-body text-center mb-1">

                    <h5 class="mt-1 mb-2">{{ambulance.driverName}}</h5>

                    <div class="md-form ml-0 mr-0">
                        <p>Contact No: {{ambulance.driverNumber}}</p>
                        <p> Ambulance No: {{ambulance.ambulanceNumber}}</p>
                        <p>Contact No: {{ambulance.gender}}</p>
                        <p>Contact No: {{ambulance.age}}</p>
                    </div>



                    <div class="text-center mt-4">
                        <button class="btn btn-cyan mt-1" id="confirmBooking" onclick="setRoutes()">Confirm Booking <i
                                class="fas fa-sign-in ml-1" data-dismiss="modal"></i></button>
                    </div>
                </div>

            </div>
            <!--/.Content-->
        </div>
